{
  "name": "swagger-schematics",
  "version": "1.0.0-alpha.2",
  "description": "Schematics types and API generator via Swagger scheme",
  "scripts": {
    "prebuild": "dtsgen --out schematics/schema.d.ts schematics/**/schema.json",
    "build": "tsc -p tsconfig.schematics.json",
    "pretest": "npm run build",
    "test": "jasmine schematics/**/*_spec.js"
  },
  "schematics": "./collection.json",
  "keywords": [
    "schematics",
    "angular",
    "generate"
  ],
  "author": "Maks Nechesonov",
  "license": "ISC",
  "dependencies": {
    "@angular-devkit/core": "15.2.8",
    "@angular-devkit/schematics": "15.2.8",
    "@schematics/angular": "15.2.8",
    "editorconfig": "1.0.2",
    "axios": "1.4.0"
  },
  "devDependencies": {
    "@types/node": "16.18.25",
    "@types/jasmine": "4.3.1",
    "@types/json-schema": "7.0.11",
    "axios-mock-adapter": "1.21.4",
    "codelyzer": "6.0.2",
    "cpx": "1.5.0",
    "dtsgenerator": "3.18.0",
    "fs-extra": "11.1.1",
    "jasmine": "4.6.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
}
