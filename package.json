{
  "name": "swagger-schematics-tool",
  "version": "0.1.0-alpha.0",
  "description": "Swagger schematics",
  "scripts": {
    "prebuild:publish": "npm run build",
    "build": "(cd projects/swagger-schematics && npm run build)",
    "build:publish": "node build-publish.js",
    "build:debug": "npm run build:publish && (cd dist/swagger-schematics && npm link)",
    "test": "(cd projects/swagger-schematics && npm run test)",
    "test:types": "(cd projects/swagger-schematics && npm run test:types)"
  },
  "keywords": [
    "schematics",
    "angular",
    "generate"
  ],
  "author": "Maks Nechesonov",
  "license": "MIT",
  "dependencies": {
    "@angular-devkit/core": "15.2.0",
    "@angular-devkit/schematics": "15.2.0",
    "@angular-devkit/schematics-cli": "15.2.0",
    "@apidevtools/json-schema-ref-parser": "9.0.9",
    "@schematics/angular": "14.0.4",
    "axios": "0.27.2",
    "rxjs": "7.5.5"
  },
  "devDependencies": {
    "@types/jasmine": "4.0.3",
    "@types/node": "14.14.41",
    "axios-mock-adapter": "1.21.1",
    "codelyzer": "6.0.2",
    "cpx": "1.5.0",
    "dtsgenerator": "3.16.0",
    "fs-extra": "10.1.0",
    "jasmine": "4.2.1",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  }
}
