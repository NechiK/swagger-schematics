    "@schematics/angular": "15.2.8",
    "editorconfig": "1.0.2",
    "axios": "1.4.0"
  },
  "devDependencies": {
    "@types/node": "16.18.25",
    "@types/jasmine": "4.3.1",
    "@types/json-schema": "7.0.11",
    "axios-mock-adapter": "1.21.4",
    "codelyzer": "6.0.2",
    "cpx": "1.5.0",
    "dtsgenerator": "3.18.0",
    "fs-extra": "11.1.1",
    "jasmine": "4.6.0",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  }
