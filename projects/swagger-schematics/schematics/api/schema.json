{
  "$schema": "http://json-schema.org/schema",
  "$id": "SwaggerApiSchema",
  "title": "Swagger Api Schema",
  "properties": {
    "path": {
      "type": "string",
      "format": "path",
      "description": "The path at which to create the store files, relative to the current workspace.",
      "visible": false
    },
    "project": {
      "type": "string",
      "description": "Generate in specific Angular CLI workspace project"
    },
    "swaggerSchemaUrl": {
      "type": "string",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": []
}
